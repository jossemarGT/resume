<!doctype html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="chrome=1">
    <meta name="generator" content="pandoc">
$for(author-meta)$
    <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
    <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(description)$
    <meta name="description" content="$description$">
$endif$
$if(keywords)$
    <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
$if(quotes)$
    <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
    <title>$title$</title>

    <link rel="stylesheet" href="stylesheets/simple-grid.min.css">
    <link rel="stylesheet" href="stylesheets/styles.css">
$for(css)$
    <link rel="stylesheet" href="stylesheets/$css$">
$endfor$
$if(math)$
    $math$
$endif$
    <script src="javascripts/display-fixes.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>

$for(header-includes)$
    $header-includes$
$endfor$
  </head>
  <body>
    <div class="container">
      <div class="row">
        <header id="main-header">
          <hgroup>
            <h1 class="title">$website_title$</h1>
            <h2 class="tag-line">$website_tagline$</h2>
          </hgroup>
        </header>
        <section id="main-content">

$for(include-before)$
$include-before$
$endfor$
$body$
$for(include-after)$
$include-after$
$endfor$

        </section>
      </div>
      <div class="row">
        <footer>
          <p>
            <small>
              <a class="no-print" href="https://github.com/$github_username$/$github_reponame$/blob/master/RESUME.md">View On GitHub</a>
              <span class="no-print">&bull;</span>
              Last revision: $date-meta$
            </small>
          </p>
        </footer>
      </div>
    </div>
    <script>
      (function(w,d){
        w.evenTables(d);
      })(window, document)
    </script>
    <!--[if !IE]><script>fixScale(document);</script><!--<![endif]-->
  </body>
</html>
